<% for(var i=0; i < users.length; i++) { %>
    <tr>
        <td>
            <%= i + 1 %>
        </td>
        <td>
            <%= users[i].rollNumber %>
        </td>
        <td>
            <%= users[i].name %>
        </td>
        <td>
            <%= users[i].email %>
        </td>
        <td>
            <%= users[i].gender %>
        </td>
        <td>
            <%= users[i].languages %>
        </td>
        <td>
            <%= users[i].githubUsername %>
        </td>
        <td>
            <a href="/update-user?id=<%= users[i]._id%>" class="btn border-shadow update" id="upd">
                <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
            </a>
            <a class="btn border-shadow delete" id="del" data-id=<%=users[i]._id %>>
                <span class="text-gradient"><i class="fas fa-times"></i></span>
            </a>
        </td>
    </tr>
    <% } %>